<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true" android:state_pressed="false" android:color="@color/colorPrimary">
        <shape android:shape="rectangle" >
            <corners android:radius="@dimen/button_radius_circle"/>
            <solid android:color="@color/colorPrimary" />
        </shape>
    </item>
    <item android:state_enabled="true" android:state_pressed="true" android:color="@color/colorPrimaryDark">
        <shape android:shape="rectangle" >
            <corners android:radius="@dimen/button_radius_circle"/>
            <solid android:color="@color/colorPrimaryDark" />
        </shape>
    </item>
    <item android:state_enabled="false" android:color="@color/colorButtonDisablePrimary">
        <shape android:shape="rectangle" >
            <corners android:radius="@dimen/button_radius_circle"/>
            <solid android:color="@color/colorButtonDisablePrimary" />
        </shape>
    </item>
</selector>